<%- include('./includes/head.ejs')%>
<link href="vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
<link href="vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
<!-- Font special for pages-->
<link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">


<link href="vendor/select2/select2.min.css" rel="stylesheet" media="all">
<link href="vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">

<!-- Main CSS-->
<link href="css/Rmain.css" rel="stylesheet" media="all">
<body>

<div class="fh5co-loader"></div>

<div id="page">

    <%- include('./includes/navigation.ejs') %>
    <header id="fh5co-header" class="fh5co-cover fh5co-cover-sm" role="banner"  style="background-image:url(images/img_bg_1.jpg);">
        <div class=""></div>
        <div class="fh5co-container">

            <div class="page-wrapper p-t-130 p-b-100 font-poppins">
                <div class="wrapper wrapper--w680">
                    <div class="card card-4">
                        <div class="card-body">
                            <% if(path === "edit-profile"){%>
                                <h2 class="title">Edit Profile</h2>
                            <%}else {%>
                                <h2 class="title"> Please Comlete Your Details to go further</h2>
                            <%}%>
                            <form method="POST" action="/first-step">

                                <hr>
                                <br>
                                <div class="input-group">
                                    <div style="text-align: center;">
                                        <label class="label"><strong>Personal Details</strong></label>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">First name</label>
                                            <input class="input--style-4" required type="text" name="FirstName"  value="" >
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Last name</label>
                                            <input class="input--style-4" required type="text" name="LastName" value="" >
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Date of Birth</label>
                                            <div class="input-group-icon">
                                                <input class="input--style-4 js-datepicker" required type="text" name="DOB"  value="" >
                                                <i class="zmdi zmdi-calendar-note input-icon js-btn-calendar"></i>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Gender</label>
                                            <div class="p-t-10">
                                                <label class="radio-container m-r-45">Male
                                                    <input type="radio"  checked  name="Gender" value='M'>
                                                    <span class="checkmark"></span>
                                                </label>
                                                <label class="radio-container">Female
                                                    <input type="radio" name="Gender"  value='F' >
                                                    <span class="checkmark"></span>
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Email</label>
                                            <input class="input--style-4" type="email" required disabled name="EmailID" value="<%if(newUserData[1]){%><%=newUserData[1]%><%}%>">
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Mobile Number</label>
                                            <input class="input--style-4" type="text" required name="MobileNO" disabled value="<%if(newUserData[0]){%><%=newUserData[0] %><%}%>">
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space input-group">

                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Occupation</label>
                                            <input class="input--style-4" type="text" required name="Occupation">
                                        </div>
                                    </div>
                                    <label class="label">Citizenship</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="Citizenship">
                                            <option selected="selected">Indian</option>
                                            <option >UK</option>
                                            <option >USA</option>
                                            <option >Other</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Address</label>
                                            <input class="input--style-4" type="text" name="Address">
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Income</label>
                                            <input class="input--style-4" type="text" required name="Income">
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">City</label>
                                            <input class="input--style-4" type="text" required name="City">
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Interest</label>
                                            <input class="input--style-4" type="text" name="Interest">
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">About Myself</label>
                                            <input class="input--style-4" type="text" required name="AboutMyself">
                                        </div>
                                    </div>
                                    <label class="label">Complexion</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="Complexion">
                                            <option selected="selected">Light Skin</option>
                                            <option>Fair Skin</option>
                                            <option>Medium Skin</option>
                                            <option>Olive Skin</option>
                                            <option>Tan Brown Skin</option>
                                            <option>Black Brown Skin</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Smoke</label>
                                            <div class="p-t-10">
                                                <label class="radio-container m-r-45">Yes
                                                    <input type="radio" checked="checked" name="Smoke" value="Yes">
                                                    <span class="checkmark"></span>
                                                </label>
                                                <label class="radio-container">No
                                                    <input type="radio" name="Smoke" value="No">
                                                    <span class="checkmark"></span>
                                                </label>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Drink</label>
                                            <div class="p-t-10">
                                                <label class="radio-container m-r-45">Yes
                                                    <input type="radio" checked="checked" name="Drink" value="Yes">
                                                    <span class="checkmark"></span>
                                                </label>
                                                <label class="radio-container">No
                                                    <input type="radio" name="Drink" value="No">
                                                    <span class="checkmark"></span>
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Weight</label>
                                            <input class="input--style-4" type="text" name="Weight">
                                        </div>
                                    </div>

                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Height</label>
                                            <input class="input--style-4" type="text" required name="Height">
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Expectations</label>
                                            <input class="input--style-4" type="text" required name="Expectations">
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Mother Tongue</label>
                                            <input class="input--style-4" type="text" required name="MotherTongue">
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <label class="label">Religion</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="Religion">
                                            <option selected="selected">Hindu</option>
                                            <option>Muslim</option>
                                            <option>Cristian</option>
                                            <option>Sikh</option>
                                            <option>Jain</option>
                                            <option>Buddha</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                    <label class="label">Martial Status</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="MartialStatus">
                                            <option selected="selected">Single</option>
                                            <option>Married</option>
                                            <option>Widowed</option>
                                            <option>Divorced</option>
                                            <option>Separated</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>

                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Any Child?</label>
                                            <input class="input--style-4" type="text" required name="ChildrenStatus">
                                        </div>
                                    </div>
                                </div>
                                <hr>
                                <br>
                                <div class="input-group">
                                    <div style="text-align: center;">
                                        <label class="label"><strong>Educational Details</strong>></label>
                                    </div>
                                </div>
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Higher Education (10)</label>
                                            <input class="input--style-4" rows="2" type="text" name="HigherEducation">
                                        </div>
                                    </div>

                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Secondary Secondary (10+2)</label>
                                            <input class="input--style-4" type="text" name="SecondaryEducation">
                                        </div>
                                    </div>
                                </div>

                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">College Degree</label>
                                            <input class="input--style-4" type="text" name="CollegeDegree">
                                        </div>
                                    </div>
                                </div>
                                <hr><br>
                                <div class="input-group">
                                    <div style="text-align: center;">
                                        <label class="label"><strong>Family Details</strong></label>
                                    </div>
                                </div>

                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Father Occupation</label>
                                            <input class="input--style-4" type="text" name="FatherOccupation">
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Mothers Occupation</label>
                                            <input class="input--style-4" type="text" name="MotherOccupation">
                                        </div>
                                    </div>
                                </div>

                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Brothers</label>
                                            <input class="input--style-4" type="text" name="Brothers">
                                        </div>
                                    </div>
                                    <div class="col-2">
                                        <div class="input-group">
                                            <label class="label">Sisters</label>
                                            <input class="input--style-4" type="text" name="Sisters">
                                        </div>
                                    </div>
                                </div>
                                <div class="row row-space input-group">
                                    <label class="label">Family Type</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="FamilyType">
                                            <option selected="selected">Nuclear Family</option>
                                            <option>Joint Family</option>
                                            <option>Step Family</option>
                                            <option>Grand Parent Family</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>




                                <div class="p-t-15">
                                    <button class="btn btn--radius-2 btn--blue" type="submit">Save</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </header>

    <br><br><br><br><br><br><br><br><br><br>
    <br><br><br><br><br><br><br><br><br><br>
    <br><br><br><br><br><br><br><br><br><br>
    <br><br><br><br><br><br><br><br><br><br>
    <br><br><br><br><br><br><br><br><br><br>
    <br><br><br><br><br><br><br><br><br><br>
    <br><br><br><br><br><br><br><br><br><br>
    <%- include('./includes/footer.ejs')%>


    <%- include('./includes/commonScripts.ejs')%>


    <script src="vendor/select2/select2.min.js"></script>


    <script src="vendor/datepicker/moment.min.js"></script>
    <script src="vendor/datepicker/daterangepicker.js"></script>

    <script src="js/Rglobal.js"></script>


</body><!-- This templates was made by Colorlib (https://colorlib.com) -->

</html>
