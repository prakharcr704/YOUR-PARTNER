<%- include('./includes/head.ejs')%>
<link href="/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
<link href="/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
<link href="/https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
<link href="/vendor/select2/select2.min.css" rel="stylesheet" media="all">
<link href="/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">
<link href="/css/Rmain.css" rel="stylesheet" media="all">
<body>
<div class="fh5co-loader"></div>
<div id="page">
    <%- include('./includes/navigation.ejs') %>
    <header id="fh5co-header" class="fh5co-cover fh5co-cover-sm" role="banner"  style="background-image:url(/images/img_bg_1.jpg);">
        <div class=""></div>
        <div class="fh5co-container">
            <div class="page-wrapper p-t-130 p-b-100 font-poppins">
                <div class="wrapper wrapper--w680">
                    <div class="card card-4">
                        <div class="card-body">
                            yourpartner ID: <%=profile.MemberID%>
                            <table>
                                <tr>
                                    <td><img src="/images/<%if(profile.Gender==="F"){%>fe<%}%>male.png" height="180px" alt="John" style="width: 180px"></td>
                                    <td> &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;</td>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Name: <%=profile.FirstName + " " + profile.LastName + " "%> <br>
                                            DOB: <%=profile.AgeDate+"/"+ profile.AgeMonth +"/" + profile.AgeYear%><br>
                                            Gender: <%if(profile.Gender=="M"){%>Male<%}else {%>Female<%}%>
                                        </span>
                                    </td>
                                </tr>
                            </table>
                            <table>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Occupation: <%=profile.Occupation%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Income: <%=profile.Income%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Citizenship: <%=profile.Citizenship%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            City: <%=profile.City%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Complexion: <%=profile.Complexion%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Height: <%=profile.Height%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Martial Status: <%=profile.MartialStatus%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Smoke: <%=profile.Smoke%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Drink: <%=profile.Drink%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Religion: <%=profile.Religion%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Mother Tongue: <%=profile.MotherTongue%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Higher Education: <%=profile.HigherEducation%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Secondary Education: <%=profile.SecondaryEducation%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            College Degree: <%=profile.CollegeDegree%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Interest: <%=profile.Interest%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            AboutMyself: <%=profile.AboutMyself%>
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <span style="font-family: 'Arial Rounded MT Bold'; font-size: 30px">
                                            Expectations: <%=profile.Expectations%>
                                        </span>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </header>
    <br><br><br><br><br>
    <%- include('./includes/footer.ejs')%>


    <%- include('./includes/commonScripts.ejs')%>

    <script>
        var password = document.getElementById("password"), confirm_password = document.getElementById("confirm_password");
        function validatePassword(){
            if(password.value != confirm_password.value) {
                confirm_password.setCustomValidity("Passwords Don't Match");
            } else {
                confirm_password.setCustomValidity('');
            }
        }

        password.onchange = validatePassword;
        confirm_password.onkeyup = validatePassword;</script>

    <script src="vendor/select2/select2.min.js"></script>


    <script src="vendor/datepicker/moment.min.js"></script>
    <script src="vendor/datepicker/daterangepicker.js"></script>

    <script src="js/Rglobal.js"></script>

</div>
