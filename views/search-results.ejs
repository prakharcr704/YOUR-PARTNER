<%- include('./includes/head.ejs') %>
</head>
<body>
<div class="fh5co-loader"></div>
<div id="page">
<%- include('./includes/navigation.ejs') %>
    <header id="fh5co-header" class="fh5co-cover fh5co-cover-sm" role="banner" style="height:100px ;background-image:url(images/img_bg_1.jpg);">

    </header>
    <br><br>
<table align="center" border = "6">
<%for(let profile of result){%>
    <tr>
        <td>
            <h1><%=profile.FirstName + " " + profile.LastName%></h1>
            <table border="3" cellpadding="100" cellspacing="2">
                <tr>
                    <td><p>YourPartner ID: <%= profile.MemberID%> </p></td>
                    <td><p>Religion: <%= profile.Religion%></p></td>
                </tr>
                <tr>
                    <td><p>Mother Tongue: <%= profile.MotherTongue%> </p></td>
                    <td><p>Height: <%= profile.Height%> </p></td>
                </tr>
                <tr>
                    <td><p>Occupation: <%= profile.Occupation%> </p></td>
                    <td><p>Citizen: <%= profile.Citizenship%></p></td>
                </tr>
            </table>
            <form action ="/GetProfile/<%=profile.MemberID%>" method="get">
                <p>&nbsp;<button>View Profile</button></p>
            </form>
        </td>
        <td >
                <img src="/images/<%if(profile.Gender==="F"){%>fe<%}%>male.png" height="180px" alt="John" style="width: 180px">
        </td>

    </tr>
<%}%>
</table>

    <%- include('./includes/footer.ejs')%>

    <%- include('./includes/commonScripts.ejs')%>


</body>
</html>